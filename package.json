    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "bun-plugin-dts": "^0.3.0",
    "react-dom": "^19.0.0",
    "release-it": "^18.1.1",
    "remark-gfm": "^4.0.0"
